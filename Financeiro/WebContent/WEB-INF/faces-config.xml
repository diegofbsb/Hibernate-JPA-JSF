<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">


	<managed-bean>
		<managed-bean-name>nomesBean</managed-bean-name>
		<managed-bean-class>br.com.algaworks.bean.NomesBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>livrosBean</managed-bean-name>
		<managed-bean-class>br.com.algaworks.bean.LivrosBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>calculadoraBean</managed-bean-name>
		<managed-bean-class>br.com.algaworks.bean.CalculadoraBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>calculadoraDataBean</managed-bean-name>
		<managed-bean-class>br.com.algaworks.bean.CalculadoraDataBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>cadastroLancamentoBean</managed-bean-name>
		<managed-bean-class>br.com.algaworks.bean.CadastroLancamentoBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	
	<managed-bean>
		<managed-bean-name>meuBean</managed-bean-name>
		<managed-bean-class>br.com.algaworks.bean.MeuBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<application>
		<message-bundle>resources.Messages</message-bundle>
	</application>
	
</faces-config>
